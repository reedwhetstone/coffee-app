{
	"name": "coffee-app",
	"private": true,
	"version": "0.0.1",
	"engines": {
		"node": "22.x"
	},
	"type": "module",
	"scripts": {
		"sync": "svelte-kit sync",
		"dev": "vite dev",
		"build": "pnpm run sync && vite build",
		"build:analyze": "pnpm run build && vite-bundle-analyzer dist/",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "pnpm run test:unit -- --run",
		"test:e2e": "pnpm exec playwright test",
		"test:e2e:ui": "pnpm exec playwright test --ui",
		"test:e2e:report": "pnpm exec playwright show-report",
		"test:e2e:auth": "pnpm exec playwright test --project=setup",
		"test:codegen": "pnpm exec playwright codegen http://localhost:5173",
		"test:codegen:auth": "pnpm exec playwright codegen --load-storage=tests/e2e/.auth/user.json http://localhost:5173",
		"vercel-dev": "vercel dev"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.3",
		"@playwright/test": "^1.58.1",
		"@rollup/rollup-darwin-arm64": "^4.56.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-vercel": "^5.6.3",
		"@sveltejs/kit": "^2.17.2",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/d3": "^7.4.3",
		"@types/lodash": "^4.17.14",
		"@types/node": "^22.10.5",
		"@types/papaparse": "^5.3.16",
		"autoprefixer": "^10.4.20",
		"bufferutil": "^4.0.9",
		"dotenv": "^16.6.1",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"mdsvex": "^0.11.2",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^5.0.0",
		"svelte-check": "^4.1.5",
		"tailwindcss": "^3.4.9",
		"ts-node": "^10.9.2",
		"tsx": "^4.19.2",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.0.0",
		"utf-8-validate": "^6.0.5",
		"vite": "^5.1.0",
		"vite-bundle-analyzer": "^0.23.0",
		"vitest": "^2.0.4"
	},
	"dependencies": {
		"@ai-sdk/openai": "^3.0.0",
		"@ai-sdk/svelte": "^4.0.0",
		"@humanspeak/svelte-markdown": "^0.8.10",
		"@supabase/ssr": "^0.5.2",
		"@supabase/supabase-js": "^2.48.1",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@vercel/analytics": "^1.4.1",
		"@vercel/speed-insights": "^1.2.0",
		"ai": "^6.0.0",
		"bcryptjs": "^3.0.2",
		"d3": "^7.9.0",
		"glob": "^10.3.10",
		"jose": "^5.9.6",
		"jwt-decode": "^4.0.0",
		"lodash": "^4.17.21",
		"papaparse": "^5.5.3",
		"playwright": "^1.58.1",
		"pnpm": "^10.28.1",
		"stripe": "^17.7.0",
		"uuid": "^9.0.1",
		"xlsx": "^0.18.5",
		"zod": "^4.0.17"
	}
}

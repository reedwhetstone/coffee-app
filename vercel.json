{
	"functions": {
		"api/**": {
			"maxDuration": 300
		}
	},
	"headers": [
		{
			"source": "/api/chat",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "no-cache, no-store, must-revalidate"
				},
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				}
			]
		}
	]
}
